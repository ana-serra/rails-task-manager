<h1> My tasks </h1>
<ul class="list-group">
  <% @tasks.each do |task| %>
  <li class="list-group-item">
    <input class="form-check-input me-1" type="checkbox" value="" aria-label="..."> <%= link_to task.title, task_path(task)%>
  <% end %>
</ul>

<i class="fa-solid fa-plus"><%= link_to "New task", new_task_path  %></i>
